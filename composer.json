{
    "name": "carte-blanche/bundle-crud",
    "type": "carte-blanche-bundle",
    "description": "The bundle to build the automatic objects from an object map",
    "keywords": [ "carte blanche", "bundles" ],
    "homepage": "http://github.com/php-carteblanche/bundle-crud",
    "license": "Apache-2.0",
    "authors": [{
        "name": "Les Ateliers Pierrot & contributors",
        "email": "contact@ateliers-pierrot.fr",
        "homepage": "http://www.ateliers-pierrot.fr/"
    }],
    "require": {
        "carte-blanche/core": "dev-cb_wip",
        "carte-blanche/installer": "dev-cb_wip"
    },
    "autoload": { 
        "psr-0": { "Crud": "src" },
        "classmap": [ "src/Crud/Controller" ]
    },
    "repositories": [{
        "type": "composer", "url": "http://packagist.ateliers-pierrot.fr/"
    }],
    "extra": {
        "views": "src/Crud/views",
        "carte-blanche-configs": [ "config/crud_config.ini", "config/int_tables.php" ],
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
